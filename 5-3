alien_colour = ['green', 'yellow', 'red']
if 'green' in alien_colour:
    print('the player just earned 5 points');

if 'yellow' in alien_colour:
    print('the player just earned 10 points');

if 'red' in alien_colour:
    print('the player just earned 20 points')

if 'orange' in alien_colour:
    print('the player just earned 30 points')
