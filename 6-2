favorite_numbers = {
    'Anna' : 7,
    'Kwadwo' : 44,
    'Candy' : 9,
    'Maureen' : 27,
}

number = favorite_numbers['Anna']
print(f"Anna's favorite number is {number}");

number = favorite_numbers['Kwadwo']
print(f"Kwadwo's favorite number is {number}");

number = favorite_numbers['Cindy']
print(f"Candy's favorite number is {number}");

number = favorite_numbers['Maureen']
print(f"Maureen's favorite number is {number}")
